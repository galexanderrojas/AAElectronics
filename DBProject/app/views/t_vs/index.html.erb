<p id="notice"><%= notice %></p>

<h1>Tvs</h1>

<table>
  <thead>
    <tr>
      <th>Tvid</th>
      <th>Name</th>
      <th>Version</th>
      <th>Model</th>
      <th>Vendor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tvs.each do |tv| %>
      <tr>
        <td><%= tv.TVid %></td>
        <td><%= tv.name %></td>
        <td><%= tv.version %></td>
        <td><%= tv.model %></td>
        <td><%= tv.vendor %></td>
        <td><%= link_to 'Show', tv %></td>
        <td><%= link_to 'Edit', edit_tv_path(tv) %></td>
        <td><%= link_to 'Destroy', tv, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tv', new_tv_path %>
