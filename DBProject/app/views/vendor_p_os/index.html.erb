<p id="notice"><%= notice %></p>

<h1>Vendor Pos</h1>

<table>
  <thead>
    <tr>
      <th>Productid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendor_pos.each do |vendor_po| %>
      <tr>
        <td><%= vendor_po.productid %></td>
        <td><%= link_to 'Show', vendor_po %></td>
        <td><%= link_to 'Edit', edit_vendor_po_path(vendor_po) %></td>
        <td><%= link_to 'Destroy', vendor_po, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor Po', new_vendor_po_path %>
